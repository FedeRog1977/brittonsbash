  "scripts": {
    "format:check": "prettier --ignore-path ./.gitignore --check .",
    "format:repair": "prettier --ignore-path ./.gitignore --list-different --write .",
    "clean": "rimraf build && rimraf ./node_modules",
    "bootstrap": "npm install && husky install",
    "start": "react-scripts start",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "eslint ./src"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:storybook/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.4.0",
    "@storybook/addon-essentials": "^8.1.3",
    "@storybook/addon-interactions": "^8.1.3",
    "@storybook/addon-links": "^8.1.3",
    "@storybook/addon-onboarding": "^8.1.3",
    "@storybook/blocks": "^8.1.3",
    "@storybook/preset-create-react-app": "^8.1.3",
    "@storybook/react": "^8.1.3",
    "@storybook/react-webpack5": "^8.1.3",
    "@storybook/test": "^8.1.3",
    "@types/leaflet": "^1.9.3",
    "@types/react-lazy-load-image-component": "^1.6.0",
    "@types/react-modal": "^3.16.0",
    "eslint-plugin-storybook": "^0.8.0",
    "gh-pages": "^5.0.0",
    "prop-types": "^15.8.1",
    "storybook": "^8.1.3",
    "webpack": "^5.89.0"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "editor.formatOnSave": false,
    "[javascript]": {
      "editor.formatOnSave": false
    }
  }
